cni:
  - canal
disable:
  - rke2-ingress-nginx
etcd-s3: true
etcd-s3-access-key: ${etcd_s3_access_key_id}
etcd-s3-bucket: ${etcd_s3_bucket}
etcd-s3-endpoint: ${etcd_s3_endpoint}
etcd-s3-region: ${etcd_s3_region}
etcd-s3-secret-key: ${etcd_s3_secret_access_key}
node-ip:
%{ for ip in node_ip ~}
  - ${ip}
%{ endfor ~}
node-label:
%{ for key, value in node_labels ~}
  - ${key}=${value}
%{ endfor ~}
protect-kernel-defaults: true
tls-san:
%{ for value in tls_sans ~}
  - ${value}
%{ endfor ~}
